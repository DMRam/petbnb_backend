# Ignore node_modules directory
node_modules/

# Ignore log files
*.log

# Ignore environment-specific files
.env
.env.local
.env.development
.env.test
.env.production

# Ignore npm's debug log
npm-debug.log*

# Ignore yarn's log files and yarn error logs
yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore VS Code settings
.vscode/

# Ignore IDE-specific files
.idea/
*.code-workspace

# Ignore macOS system files
.DS_Store

# Ignore testing directories
__tests__/
test/
spec/

# Ignore coverage reports
coverage/

# Ignore dependency lock files
package-lock.json
yarn.lock

# Ignore editor backup files
*~

# Ignore Google credentials
*.json # Assuming Google credentials are stored in JSON files
app.yaml
